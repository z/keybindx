<?xml version='1.0' standalone='yes'?>
<nkml>
	<title>Ninja Pack v1 - Nexuiz Ninjaz</title>
	<software>Nexuiz</software>
	<lang>en</lang>
	<author>Tyler "-z-" Mulligan</author>
	<website>www.nexuizninjaz.com</website>
	<keyboard>
		<function_keys>
			<key id="key_esc" title="Menu" action="togglemenu" color="FAB07F" />
			<key id="key_f1" title="Vote Yes" action="vote yes" color="FAB07F" />
			<key id="key_f2" title="Vote No" action="vote no" color="FAB07F" />
			<key id="key_f3" title="Spectate" action="magic" color="FAB07F" />
			<key id="key_f4" title="" />
			<key id="key_f5" title="pick team" action="menu_show" color="FAB07F" />
			<key id="key_f6" title="switch teams" action="team_auto" color="FAB07F" />
			<key id="key_f7" title="" />
			<key id="key_f8" title="" />
			<key id="key_f9" title="toggle fullscreen" action="toggle_fullscreen" color="FAB07F" />
			<key id="key_f10" title="quit" action="quit" color="FAB07F" />
			<key id="key_f11" title="" />
			<key id="key_f12" title="screenshot" action="screenshot" color="FAB07F" />
		</function_keys>
		<row>
			<key id="key_backtick" title="Console" action="toggleconsole" color="FAB07F" />
			<key id="key_1" title="I need help" action="nn_help" color="C2DBF2" />
			<key id="key_2" title="Attacking" action="nn_attacking" color="C2DBF2" />
			<key id="key_3" title="Defending" action="nn_defending" color="C2DBF2" />
			<key id="key_4" title="I'm on my way" action="nn_on_my_way" color="C2DBF2" />
			<key id="key_5" title="Sorry player" action="say_team sorry %p" color="BAB0BF" />
			<key id="key_6" title="Thanks" action="say sorry %p" color="BAB0BF" />
			<key id="key_7" title="" />
			<key id="key_8" title="" />
			<key id="key_9" title="" />
			<key id="key_0" title="" />
			<key id="key_minus" title="" />
			<key id="key_plus" title="" />
			<key id="key_backspace" title="" />
		</row>
		<row>
			<key id="key_tab" title="Score" action="+showscores" color="FAB07F" />
			<key id="key_q" title="Rocket" action="bestweapon 9 4 8" color="98D1A9" />
			<key id="key_w" title="Forward" action="+forward" color="D0DF59" />
			<key id="key_e" title="Laser" action="impulse 1" color="98D1A9" />
			<key id="key_r" title="MG" action="bestweapon 3 6 5 8" color="98D1A9" />
			<key id="key_t" title="Chat" action="messagemode" color="BAB0BF" />
			<key id="key_y" title="Team Chat" action="messagemode2" color="C2DBF2" />
			<key id="key_u" title="" />
			<key id="key_i" title="" />
			<key id="key_o" title="" />
			<key id="key_p" title="" />
			<key id="key_left_bracket" title="" />
			<key id="key_right_bracket" title="" />
			<key id="key_backslash" title="" />
		</row>
		<row>
			<key id="key_caps_lock" title="" color="111" />
			<key id="key_a" title="Left" action="+moveleft" color="D0DF59" />
			<key id="key_s" title="Back" action="+back" color="D0DF59" />
			<key id="key_d" title="Right" action="+moveright" color="D0DF59" />
			<key id="key_f" title="Nex" action="bestweapon 7 3 1" color="98D1A9" />
			<key id="key_g" title="Drop Weapon" action="dropweapon" color="55BA63" />
			<key id="key_h" title="" />
			<key id="key_j" title="" />
			<key id="key_k" title="Hello!" action="say '\m/ (-.-) \m/ Hello'" color="BAB0BF" />
			<key id="key_l" title="Goodbye!" action="say 'Goodbye \m/ (-.-) \m/'" color="BAB0BF" />
			<key id="key_semicolon" title="" />
			<key id="key_apostrophe" title="change crosshair size" action="toggle crosshair_size 4 3.5 3 3 2.5 2 1.5 1 0.5 0.3 0.1 0; echo Crosshair size $crosshair_size" color="FAB07F" />
			<key id="key_enter" title="" />
		</row>
		<row>
			<key id="key_left_shift" title="Crouch" action="+crouch" color="D0DF59" />
			<key id="key_z" title="Zoom/FOV Adjust" action="+aot_zoomadjust" color="FFD75F" />
			<key id="key_x" title="FOV Reset" action="fov 130" color="FFD75F" />
			<key id="key_c" title="Electro" action="bestweapon 5 4 8" color="98D1A9" />
			<key id="key_v" title="Mortar" action="bestweapon 4 5 8" color="98D1A9" />
			<key id="key_b" title="" />
			<key id="key_n" title="good job team(mate)!" action="say_team good job team/player!" color="C2DBF2" />
			<key id="key_m" title="" />
			<key id="key_comma" title="" />
			<key id="key_period" title="" />
			<key id="key_forwardslash" title="" />
			<key id="key_right_shift" title="" />
		</row>
		<row>
			<key id="key_left_ctrl" title="Nevermind"  action="nn_nevermind" color="C2DBF2" />
			<key id="key_left_special" title="" color="111" />
			<key id="key_left_alt" title="" />
			<key id="key_space_bar" title="Jump" action="+jump" color="D0DF59" />
			<key id="key_right_alt" title="" />
			<key id="key_right_special" title="" color="111" />
			<key id="key_popup" title="" color="111" />
			<key id="key_right_ctrl" title="" />
		</row>
		<center_chunk>
			<key id="key_prtscr" title="" />
			<key id="key_scrlck" title="" />
			<key id="key_pause" title="" />
			<key id="key_insert" title="" />
			<key id="key_home" title="" />
			<key id="key_page_up" title="Ninja Binds" action="exec nn_keybinds.cfg" color="F58D80" />
			<key id="key_delete" title="" />
			<key id="key_end" title="" />
			<key id="key_page_down" title="Video Binds" action="exec nn_video.cfg" color="F58D80" />
			<key id="key_up" title="" />
			<key id="key_left" title="" />
			<key id="key_down" title="" />
			<key id="key_right" title="" />
		</center_chunk>
		<numpad>
			<key id="key_num_lock" title="" />
			<key id="key_num_slash" title="dropped flag" action="say_team ^1Dropped Flag ^7(^4l:^5%d^7); g_waypointsprite_team_here_d" color="C2DBF2" />
			<key id="key_num_multiply" title="Killed FC" action="say_team ^2Killed Flagcarrier ^7(^4l:^5%y^7); g_waypointsprite_team_p" color="C2DBF2" />
			<key id="key_num_minus" title="hi / gl" action="say hi / good luck and have fun (-.-) \m/" color="BAB0BF" />
			<key id="key_num_7" title="Defending" action="say_team ^5Defending ^7(^4l:^5%l^7) ^7(^1h:%h^7 ^2a:%a^7 ^6w:%w^7); g_waypointsprite_team_here" color="C2DBF2" />
			<key id="key_num_8" title="Roaming" action="say_team ^5Roaming ^7(^4l:^5%l^7) ^7(^1h:%h^7 ^2a:%a^7 ^6w:%w^7); g_waypointsprite_team_here" color="C2DBF2" />
			<key id="key_num_9" title="Attacking" action="say_team ^5Attacking ^7(^4l:%l^7) ^7(^1h:%h^7 ^2a:%a^7 ^6w:%w^7); g_waypointsprite_team_here" color="C2DBF2" />
			<key id="key_num_plus" title="Good Game" action="say Good Game!" color="BAB0BF" />
			<key id="key_num_6" title="Enemy Seen" action="say_team ^1Enemy Seen ^7(^4l:^5%y^7); g_waypointsprite_team_danger_p" color="C2DBF2" />
			<key id="key_num_5" title="Need Help" action="say_team ^3Need Help ^7(^4l:^5%l^7) ^7(^1h:%h^7 ^2a:%a^7 ^6w:%w^7); g_waypointsprite_team_helpme" color="C2DBF2" />
			<key id="key_num_4" title="Negative" action="say_team ^1Negative" color="C2DBF2" />
			<key id="key_num_1" title="Quad Soon" action="say_team ^4Quad Soon" color="C2DBF2" />
			<key id="key_num_2" title="Free Item" action="say_team ^6Free Item %x ^7(^4l:^5%y^7); g_waypointsprite_team_here_p" color="C2DBF2" />
			<key id="key_num_3" title="Took Item" action="say_team ^6Took Item ^7(^4l:^5%l^7); g_waypointsprite_team_here" color="C2DBF2" />
			<key id="key_num_enter" title="Nice One" action="say :-) / nice on" color="BAB0BF" />
			<key id="key_num_period" title="teamchat" action="messagemode2" color="C2DBF2" />
			<key id="key_num_0" title="chat" action="messagemode" color="BAB0BF" />
		</numpad>
	</keyboard>
	<mouse>
		<button id="mouse_1" title="Primary Fire" action="+fire" />
		<button id="mouse_4" title="Hook" action="+hook" />
		<button id="mouse_5" title="CTF Menu" action="+aot_menu" />
		<button id="mouse_3" title="Zoom" action="+aot_temp_zoom" />
		<button id="mouse_2" title="Secondary Fire" action="+fire2" />
		<button id="mouse_6" title="" />
		<button id="mouse_7" title="" />
		<button id="mouse_wheel_up" title="Previous Weapon" action="prevweapon" />
		<button id="mouse_wheel_down" title="Next Weapon" action="nextweapon" />
	</mouse>
</nkml>
